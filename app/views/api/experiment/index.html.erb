<h1>Listing Experiments</h1>

<table>
  <thead>
  <tr>
    <th>Email</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Phone number</th>
    <th>Address 1</th>
    <th>Address 2</th>
    <th>Address 3</th>
    <th>Country</th>
    <th>Token</th>
    <th>Ip address</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @experiments.each do |experiment| %>
    <tr>
      <td><%= experiment.page %></td>
      <td><%= experiment.site_domain %></td>
      <td><%= experiment.content %></td>
      <td><%= experiment.user.first_name %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

